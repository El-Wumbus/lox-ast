#include("test.file")
#define HELLO "Hello"

// A global variable, evaluated at compile time. A constant value
const i32 result = 1;

// A global variable, evaluated at runtime. A constant value
static i32 result2 = 2;

// A global variable, evaluated at runtime. Available from other files
pub static i32 result3 = 3;

i32 main()
{
    // For implicit typing use `let`. 
    let args = std::ProgramArgs::get().unwrap();

    print("First arg: {}, Number of args:{}\n", args.vector[0], args.count);
}
